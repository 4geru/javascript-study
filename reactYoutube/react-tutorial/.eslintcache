list')}}>リスト</HeaderLi>\n          <HeaderLi focused={tab === 'form' } onClick={() => {setTab('form')}}>フォーム</HeaderLi>\n          <HeaderLi focused={tab === 'table' } onClick={() => {setTab('table')}}>テーブル</HeaderLi>\n        </HeaderUl>\n      </Header>\n      {\n        tab === 'list' ?\n          <List languages={langs} /> :\n          tab === 'form' ?\n            <Form onAddLang={addLang} /> :\n            <Table languages={langs} onDeleteLang={deleteLang} />\n      }\n    </div>\n  );\n}\n\nexport default withLoading(App, getLanguages);\n","/Users/4geru/train/study/reactYoutube/react-tutorial/src/List.js",[],"/Users/4geru/train/study/reactYoutube/react-tutorial/src/index.js",[],"/Users/4geru/train/study/reactYoutube/react-tutorial/src/Form.js",[],"/Users/4geru/train/study/reactYoutube/react-tutorial/src/Table.js",[],"/Users/4geru/train/study/reactYoutube/react-tutorial/src/const/languages.js",[],"/Users/4geru/train/study/reactYoutube/react-tutorial/src/components/button.js",[],"/Users/4geru/train/study/reactYoutube/react-tutorial/src/hoc/withLoading.js",[],"/Users/4geru/train/study/reactYoutube/react-tutorial/src/components/tab-body-container.js",[],{"ruleId":"44","severity":1,"message":"45","line":1,"column":20,"nodeType":"46","messageId":"47","endLine":1,"endColumn":29},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar"]